<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd"
           version="3.0">
    <servlet>
        <servlet-name>RegisteringForm</servlet-name>
        <jsp-file>/RegisteringForm.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisteringForm</servlet-name>
        <url-pattern>/RegisteringForm</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProfilePage</servlet-name>
        <jsp-file>/ProfilePage.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProfilePage</servlet-name>

        <url-pattern>/ProfilePage</url-pattern>

    </servlet-mapping>

</web-app>

